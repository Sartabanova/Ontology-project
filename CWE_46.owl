    Annotations: 
        rdfs:comment "The SalesCart shopping cart application allows remote users to modify sensitive purchase information via hidden form fields."^^xsd:string
    
    Types: 
        CVEEntries
    
    
Individual: CVE-2000-0172

    Annotations: 
